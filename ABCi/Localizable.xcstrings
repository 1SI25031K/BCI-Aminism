    "shikigami.p1" : {
      "comment" : "A text displayed below the title of the view.",
      "isCommentAutoGenerated" : true,
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "The Shikigami (式神) or \"ceremonial spirit\" is not a natural entity, but a spirit \"born of and bound by human ritual\". Originating from Onmyōdō (the Way of Yin and Yang) , it is the quintessential tool of its master, the onmyōji (sorcerer)."
          }
        }
      }
    },
    "shikigami.p2" : {
      "comment" : "A paragraph describing the significance of the Shikigami in the story.",
      "isCommentAutoGenerated" : true,
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "A Shikigami is defined by its \"complete lack of agency\". It is, in blunt terms, a \"slave\" and a form of \"spiritual automation\" that serves only its master's commands. They are often bound to a physical vessel, such as a katashiro, or paper manikin."
          }
        }
      }
    },
